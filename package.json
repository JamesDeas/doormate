{
  "name": "doormate",
  "version": "1.0.0",
  "description": "Door Engineers App with React Native Expo frontend and Node.js backend",
  "scripts": {
    "frontend": "cd frontend && npm run dev",
    "backend": "cd backend && npm run dev",
    "dev": "concurrently --kill-others --names \"BACKEND,FRONTEND\" --prefix-colors \"blue.bold,green.bold\" --prefix \"[{name}]\" \"npm run backend\" \"npm run frontend\"",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "react-native",
    "expo",
    "nodejs",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "expo-file-system": "^18.0.12",
    "fs-extra": "^11.3.0",
    "multer": "^1.4.5-lts.2",
    "openai": "^4.87.3",
    "react-native-blob-util": "^0.21.2",
    "react-native-pdf": "^6.7.7"
  }
}
